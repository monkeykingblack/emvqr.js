{
  "name": "emvqr.js",
  "version": "1.1.0",
  "description": "Javascript library to work with EMV Compliant QR code",
  "type": "module",
  "types": "./mjs/index.d.ts",
  "main": "./cjs/index.js",
  "module": "./mjs/index.js",
  "exports": {
    ".": {
      "types": "./mjs/index.d.ts",
      "import": "./mjs/index.js",
      "require": "./cjs/index.js"
    }
  },
  "files": [
    "cjs",
    "mjs",
    "*.js",
    "*.d.ts"
  ],
  "author": "d.q.t.7775@gmail.com",
  "license": "MIT",
  "dependencies": {
    "fluent-ts-validator": "^3.0.3"
  }
}
